@page "/"

@inherits DashboadBase

<h3>Top Heroes</h3>
<div class="grid grid-pad">
    @foreach (var hero in topFive)
    {
        <a class="col-1-4" href="/HeroDetails/@hero.Id">
            <div class="module hero">
                <h4>@hero.Name</h4>
            </div>
        </a>
    }
</div>

<HeroSearch />
